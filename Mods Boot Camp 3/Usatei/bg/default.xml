	<Layer File="bgp1"
		OnCommand="%function(self)
			self:horizalign('right');
			self:vertalign('bottom');
			self:x(SCREEN_WIDTH);
			self:y(SCREEN_HEIGHT*.5);
			self:zoomtoheight(SCREEN_HEIGHT);
			self:zoomtowidth(SCREEN_WIDTH+696);
			self:queuecommand('Update')
			usa_bgp1 = self
		end"
		UpdateCommand="x,SCREEN_WIDTH;linear,0.2;addx,696;sleep,0;queuecommand,Update" />
