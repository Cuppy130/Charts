<ActorFrame> <children>

	<Layer
		Type="Quad"
		OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;zoomtowidth,SCREEN_WIDTH;zoomtoheight,SCREEN_HEIGHT;diffuse,0,.2,.6,.4"
	/>
	
	<Layer
		File="whee"
		OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;diffuse,1,1,1,0;blend,add;playcommand,Name"
		NameCommand="%function(self) fd_da_whee = self end"
	/>
	
	<Layer File="../lua/marsface1" InitCommand="%function(self)
			fd_da_face = self;
			self:x(SCREEN_CENTER_X)
			self:y(SCREEN_CENTER_Y)
		end"
	/>
	<Layer File="../lua/marsface2" InitCommand="%function(self)
			fd_da_face2 = self;
			self:x(SCREEN_CENTER_X)
			self:y(SCREEN_CENTER_Y)
			self:blend('add')
		end"
	/>
	<Layer File="../lua/blossoms3" InitCommand="%function(self)
			self:hidden(0);
			self:x(SCREEN_CENTER_X)
			self:y(SCREEN_CENTER_Y)
			self:zoomtoheight(SCREEN_HEIGHT*1.3)
			self:zoomtowidth(SCREEN_WIDTH)
			self:blend('add')
		end"
	/>
</children></ActorFrame>