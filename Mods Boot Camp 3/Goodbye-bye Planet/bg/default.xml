<ActorFrame OnCommand="playcommand,Name"> <children>

	<Layer Type="ActorFrame" OnCommand="x,SCREEN_WIDTH/2;y,SCREEN_HEIGHT/2;queuecommand,Set;" SetCommand="%function(self) ullu_landscape = self end" ><children>
	<Layer Type="ActorFrame" OnCommand="x,-SCREEN_WIDTH/2;y,-SCREEN_HEIGHT/2" ><children>
	
		<Layer
			File="sky_0"
			NameCommand="%function(self) ullu_sky_0 = self end"
			OnCommand="stretchto,-100,-20,SCREEN_WIDTH+100,SCREEN_HEIGHT+20;addy,-20;"
		/>
		
		<Layer
			File="sky_1"
			NameCommand="%function(self) ullu_sky_1 = self end"
			OnCommand="stretchto,-100,-20,SCREEN_WIDTH+100,SCREEN_HEIGHT+20;addy,-20;"
		/>
		
		<Layer
			File="sky_2"
			NameCommand="%function(self) ullu_sky_2 = self end"
			OnCommand="stretchto,-100,-20,SCREEN_WIDTH+100,SCREEN_HEIGHT+20;addy,-20;"
		/>
		
		<Layer
			File="sky_3"
			NameCommand="%function(self) ullu_sky_3 = self end"
			OnCommand="stretchto,-100,-20,SCREEN_WIDTH+100,SCREEN_HEIGHT+20;addy,-20;"
		/>
		
		<Layer
			File="sky_4"
			NameCommand="%function(self) ullu_sky_4 = self end"
			OnCommand="stretchto,-100,-20,SCREEN_WIDTH+100,SCREEN_HEIGHT+20;addy,-20;"
		/>
		
		<Layer
			File="stars"
			NameCommand="%function(self) ullu_skystars = self end"
			OnCommand="x,SCREEN_WIDTH/2;y,0;blend,add;customtexturerect,0,0,3,3;zoom,4;texcoordvelocity,0,-.05"
		/>
		
		<Layer
			File="pixel_stars"
			NameCommand="%function(self) pixel_ullu_skystars = self end"
			OnCommand="x,SCREEN_WIDTH/2;y,0;blend,add;customtexturerect,0,0,4,4;zoom,4;texcoordvelocity,0,0"
		/>
		
		<Layer
			File="sun.png"
			NameCommand="%function(self) ullu_sun = self end"
			OnCommand="zoom,1;"
		/>
		
		<Layer
			File="sunb.png"
			NameCommand="%function(self) ullu_sunb = self end"
			OnCommand="zoom,1;"
		/>
		
		<Layer
			File="pixel_sun.png"
			NameCommand="%function(self) pixel_ullu_sun = self end"
			OnCommand="zoom,1;"
		/>
		
		<Layer
			File="mountain1"
			NameCommand="%function(self) ullu_mountain1 = self end"
			OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y - 20;zoom,SCREEN_HEIGHT/480;diffusealpha,1"
		/>
		
		<Layer
			File="mountain2"
			NameCommand="%function(self) ullu_mountain2 = self end"
			OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y - 20;zoom,SCREEN_HEIGHT/480;diffusealpha,0"
		/>
		
		<Layer
			File="pixel_mountain1"
			NameCommand="%function(self) pixel_ullu_mountain1 = self end"
			OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y - 20;zoom,SCREEN_HEIGHT/480;diffusealpha,1"
		/>
		
		<Layer
			File="pixel_mountain2"
			NameCommand="%function(self) pixel_ullu_mountain2 = self end"
			OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y - 20;zoom,SCREEN_HEIGHT/480;diffusealpha,0"
		/>
		
		<Layer
			File="treeline2"
			NameCommand="%function(self) ullu_treeline2 = self end"
			OnCommand="x,SCREEN_CENTER_X-30;y,SCREEN_CENTER_Y;zoom,SCREEN_HEIGHT/480;diffusealpha,1;croptop,.08;diffuse,0,.1,.2,1;"
		/>
		
		<Layer
			Type="Quad"
			NameCommand="%function(self) ullu_treequad = self end"
			OnCommand="stretchto,-100,SCREEN_HEIGHT*0.8+5,SCREEN_WIDTH+100,SCREEN_HEIGHT+100;diffuse,0,.05,.1,1;"
		/>
		
		<Layer
			File="treeline1"
			NameCommand="%function(self) ullu_treeline1 = self end"
			OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;zoom,SCREEN_HEIGHT/480;diffusealpha,1;croptop,.08;diffuse,0,.05,.1,1;"
		/>
		
		<Layer
			File="pixel_treeline2"
			NameCommand="%function(self) pixel_ullu_treeline2 = self end"
			OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;zoom,SCREEN_HEIGHT/480;diffusealpha,1;croptop,.08;diffuse,0,.1,.2,1;"
		/>
		
		<Layer
			File="pixel_treeline1"
			NameCommand="%function(self) pixel_ullu_treeline1 = self end"
			OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;zoom,SCREEN_HEIGHT/480;diffusealpha,1;croptop,.08;diffuse,0,.05,.1,1;"
		/>
		
		<Layer File="ullu" />
		
		<Layer
			Type="Quad"
			NameCommand="%function(self) ullu_day_diffuse = self end"
			OnCommand="stretchto,-100,-100,SCREEN_WIDTH+100,SCREEN_HEIGHT+100;diffuse,0,0,0,1;"
		/>
	
	</children></Layer>
	</children></Layer>

	<Layer
		File="background"
		NameCommand="%function(self) ullu_itg = self end"
		OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;basezoomx,SCREEN_WIDTH/640;basezoomy,SCREEN_HEIGHT/480;diffuse,.5,.5,.5,1;"
	/>

</children> </ActorFrame>