
	<!-- Dyscontrolled -->
	<Layer Type="ActorFrame" InitCommand="hidden,1" RiseUpMessageCommand="accelerate,spb*7;addy,SCREEN_HEIGHT*1.25" OpenSesaMessageCommand="hidden,0"> <children>
	
