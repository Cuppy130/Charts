			BlackFlashCommand="diffuse,0,0,0,1;zoom,1;linear,180/200;diffuse,0,0,0,0;zoom,1.5;sleep,0;zoom,1;"
            FadingCommand="linear,5;diffusealpha,0"
		/>
		
	</children>	</Layer>
	
</children></ActorFrame>