<ActorFrame OnCommand="fov,50;" > <children>

	<Layer
		File="background"
		OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;ZoomToWidth,SCREEN_WIDTH;ZoomToHeight,SCREEN_HEIGHT;diffusealpha,0.5;"
	/>
	
	
	
<!-- 	<Layer
		Type="Quad" InitCommand="%function(self) aya_bgquad=self end"
		OnCommand="diffusealpha,0;stretchto,0,0,SCREEN_WIDTH,SCREEN_HEIGHT;"
	/> -->

	<Layer
		File="vignette" InitCommand="%function(self) aya_bgquad=self end"
		OnCommand="diffusealpha,0;stretchto,0,0,SCREEN_WIDTH,SCREEN_HEIGHT;"
		FlashXCommand="finishtweening;zoomtowidth,SCREEN_WIDTH*2;decelerate,0.4;zoomtowidth,SCREEN_WIDTH;"
		FlashYCommand="finishtweening;zoomtoheight,SCREEN_HEIGHT*2;decelerate,0.4;zoomtoheight,SCREEN_HEIGHT;"
		FinishCommand="linear,1;glow,1,1,1,1;"
	/>



	<Layer File="ykbg" NameCommand="%function(self) aya_bg = self self:zoom(1.2) self:bounce() self:effectclock('bgm') self:effectperiod(3) self:effectmagnitude(0,0,10) end" OnCommand="playcommand,Name;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;ZoomToWidth,SCREEN_WIDTH;ZoomToHeight,SCREEN_HEIGHT;diffusealpha,0;" />
	
	
	<Layer
		File="hole" InitCommand="%function(self) aya_hole1=self end"
		OnCommand="x,SCREEN_CENTER_X-160;y,SCREEN_BOTTOM;zoom,0;rotationx,90;zwrite,1;blend,noeffect;"
	/>
	<Layer
		File="hole" InitCommand="%function(self) aya_hole2=self end"
		OnCommand="x,SCREEN_CENTER_X+160;y,SCREEN_BOTTOM;zoom,0;rotationx,90;zwrite,1;blend,noeffect;"
	/>
	<Layer
		File="hole" InitCommand="%function(self) aya_hole3=self end"
		OnCommand="x,0;y,SCREEN_CENTER_Y+40;zoom,0;basezoomy,1.2;rotationy,90;rotationx,90;zwrite,1;blend,noeffect;"
	/>
	<Layer
		File="hole" InitCommand="%function(self) aya_hole4=self end"
		OnCommand="x,SCREEN_WIDTH;y,SCREEN_CENTER_Y+40;zoom,0;basezoomy,1.2;rotationy,-90;rotationx,90;zwrite,1;blend,noeffect;"
	/>
	<Layer
		File="hole" InitCommand="%function(self) aya_hole5=self end"
		OnCommand="x,SCREEN_CENTER_X-160;y,SCREEN_BOTTOM;zoom,0;rotationx,90;zwrite,1;blend,noeffect;"
	/>
	<Layer
		File="hole" InitCommand="%function(self) aya_hole6=self end"
		OnCommand="x,SCREEN_CENTER_X+160;y,SCREEN_BOTTOM;zoom,0;rotationx,90;zwrite,1;blend,noeffect;"
	/>
	<Layer
		File="hole" InitCommand="%function(self) aya_hole7=self end"
		OnCommand="x,SCREEN_CENTER_X-160;y,SCREEN_BOTTOM;zoom,0;rotationx,90;zwrite,1;blend,noeffect;"
	/>
	<Layer
		File="hole" InitCommand="%function(self) aya_hole8=self end"
		OnCommand="x,SCREEN_CENTER_X+160;y,SCREEN_BOTTOM;zoom,0;rotationx,90;zwrite,1;blend,noeffect;"
	/>
	
	<Layer Type="Quad" InitCommand="%function(self) aya_gap=self end"
		OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;zoomto,0,SCREEN_HEIGHT;zwrite,1;blend,noeffect;"
	/>
	
	<Layer
		File="eyes"
		OnCommand="customtexturerect,0,0,4,4;zoom,3*(SCREEN_WIDTH/640);texcoordvelocity,.4,.2;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;ztestmode,writeonfail;"
	/>
	
	
	<Layer Type="Quad" OnCommand="diffusealpha,0;stretchto,0,0,SCREEN_WIDTH,SCREEN_HEIGHT;clearzbuffer,1;" />
	
	<Layer Type="ActorFrame" OnCommand="%function(self) self:bob() self:effectmagnitude(0,10,0) self:effectclock('bgm') self:effectperiod(3) end"><children>
		<Layer File="Yukari" NameCommand="%function(self) aya_yukari = self end" OnCommand="playcommand,Name;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;hidden,1;" />
	</children></Layer>
	
</children> </ActorFrame>