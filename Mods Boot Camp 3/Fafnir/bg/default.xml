<ActorFrame> <children>

	<Layer Type="ActorFrame" OnCommand="fov,60" Condition="not FUCK_EXE"><children>
		
		<Layer Type="Quad" OnCommand="diffuse,0,0,0,1;stretchto,0,0,sw,sh" />
		
		
		<Layer File="../lua/sky" InitCommand="%function(self) faf_defeatsky = self end" OnCommand="stretchto,0,0,sw,sh;diffusealpha,0;" />
		
		<Layer File="../lua/clouds" InitCommand="%function(self) faf_defeatclouds = self end" OnCommand="customtexturerect,0,0,3,3;x,sw/2;zoom,6;fadebottom,.5;y,0;rotationx,60;texcoordvelocity,0,.1;diffusealpha,0;" />
		
		<Layer File="../lua/treeline" InitCommand="%function(self) faf_defeattrees = self end" OnCommand="x,sw/2;zoom,.8;diffuse,1*.6,.8*.4,.6*.4,0;y,sh-100;" />
		
		<Layer File="../lua/pandora1" InitCommand="%function(self) faf_spooky2 = self end" OnCommand="scaletofit,0,0,sw,sh;diffusealpha,0;" />
		<Layer File="../lua/sad_panda" InitCommand="%function(self) faf_defeat2 = self end" OnCommand="scaletofit,0,0,sw,sh;diffusealpha,0;" />
		
		<Layer File="../lua/peaceful_v" InitCommand="%function(self) faf_defeatvig = self end" OnCommand="stretchto,0,0,sw,sh;diffusealpha,0;" />
		
		<Layer File="../lua/ok" InitCommand="%function(self) faf_spooky1 = self end" OnCommand="stretchto,0,0,sw,sh;diffusealpha,0;" />
		<Layer File="../lua/ok" InitCommand="%function(self) faf_defeat1 = self end" OnCommand="stretchto,0,0,sw,sh;diffusealpha,0;rotationz,180" />
		
		<Layer Type="Quad" InitCommand="%function(self) faf_bgwhite = self end" OnCommand="stretchto,-sw*40,-sh*40,sw*40,sh*40;diffuse,1,1,1,0;" />
		
		
	</children></Layer>

	<Layer
		File="itg" NameCommand="%function(self) faf_bg1 = self end"
		OnCommand="playcommand,Name;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;ZoomToWidth,SCREEN_WIDTH;ZoomToHeight,SCREEN_HEIGHT;diffuse,.5,.5,.5,1"
	/>
	<Layer
		File="background2" NameCommand="%function(self) faf_bg2 = self end"
		OnCommand="playcommand,Name;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;ZoomToWidth,SCREEN_WIDTH;ZoomToHeight,SCREEN_HEIGHT;diffuse,.7,.7,.7,0"
	/>
	<Layer Type="ActorFrame" OnCommand="fov,60"><children>
	<Layer
		File="dark" NameCommand="%function(self) faf_bg3 = self end"
		OnCommand="playcommand,Name;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;ZoomToWidth,SCREEN_WIDTH;ZoomToHeight,SCREEN_HEIGHT;zoomz,SCREEN_WIDTH/640;diffuse,1,1,1,0"
	/>
	</children></Layer>
	<Layer
		Type="Quad" NameCommand="%function(self) faf_black = self end"
		OnCommand="playcommand,Name;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;ZoomToWidth,SCREEN_WIDTH;ZoomToHeight,SCREEN_HEIGHT;diffuse,0,0,0,0;"
	/>
	
	<Layer Type="ActorFrame" NameCommand="%function(self) faf_bubble1 = self end"
		OnCommand="playcommand,Name;" ><children>
		<Layer File="glow" OnCommand="zoom,.5;diffusealpha,0;spin;effectmagnitude,0,0,50;"
		UpCommand="bounceend,60/196;diffusealpha,.6;zoom,1;"
		Up2Command="linear,120/196;zoom,.4;bounceend,60/196;diffusealpha,.6;zoom,1;"
		DownCommand="bouncebegin,60/196;zoom,.5;diffusealpha,0;" />
		<Layer File="bubble" OnCommand="zoom,.5;diffusealpha,0;spin;effectmagnitude,0,0,-38;"
		UpCommand="diffusealpha,.2;bounceend,60/196;diffusealpha,.6;zoom,1;"
		Up2Command="linear,120/196;zoom,.4;diffusealpha,.2;bounceend,60/196;diffusealpha,.6;zoom,1;"
		DownCommand="bouncebegin,60/196;zoom,.5;diffusealpha,0;" />
		<Layer File="eye" OnCommand="y,10;diffusealpha,0;zoom,.5;basezoomx,2;basezoomy,2;"
		WarnCommand="linear,480/196;diffusealpha,2;zoom,1;" UpCommand="stoptweening;linear,0.5;diffusealpha,0;zoom,0.5;" Up2Command="stoptweening;linear,0.5;diffusealpha,0;zoom,0.5;" />
	</children></Layer>
	<Layer Type="ActorFrame" NameCommand="%function(self) faf_bubble2 = self end"
		OnCommand="playcommand,Name;" ><children>
		<Layer File="glow" OnCommand="zoom,.5;diffusealpha,0;spin;effectmagnitude,0,0,-50;"
		UpCommand="bounceend,60/196;diffusealpha,.6;zoom,1;"
		Up2Command="linear,120/196;zoom,.4;bounceend,60/196;diffusealpha,.6;zoom,1;"
		DownCommand="bouncebegin,60/196;zoom,.5;diffusealpha,0;" />
		<Layer File="bubble" OnCommand="zoom,.5;diffusealpha,0;spin;effectmagnitude,0,0,38;"
		UpCommand="diffusealpha,.2;bounceend,60/196;diffusealpha,.6;zoom,1;"
		Up2Command="linear,120/196;zoom,.4;diffusealpha,.2;bounceend,60/196;diffusealpha,.6;zoom,1;"
		DownCommand="bouncebegin,60/196;zoom,.5;diffusealpha,0;" />
		<Layer File="eye" OnCommand="y,10;diffusealpha,0;zoom,.5;basezoomx,2;basezoomy,2;"
		WarnCommand="linear,480/196;diffusealpha,2;zoom,1;" UpCommand="stoptweening;linear,0.5;diffusealpha,0;zoom,0.5;" Up2Command="stoptweening;linear,0.5;diffusealpha,0;zoom,0.5;" />
	</children></Layer>

</children> </ActorFrame>