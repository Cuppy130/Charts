<ActorFrame> <children>

	<Layer
		File="background"
		OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;ZoomToWidth,SCREEN_WIDTH;ZoomToHeight,SCREEN_HEIGHT;diffusealpha,0.5"
	/>
	
	<Layer
		Type="Quad" NameCommand="%function(self) rb_bgquad=self end"
		OnCommand="playcommand,Name;diffusealpha,0;stretchto,0,0,SCREEN_WIDTH,SCREEN_HEIGHT;diffuse,0,0,0,0;"
	/>
	<Layer
		File="stg7bg2" NameCommand="%function(self) rb_bg2=self end"
		OnCommand="playcommand,Name;diffusealpha,0;customtexturerect,0,0,3,3;zoom,4;texcoordvelocity,.2,.2;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;"
	/>
	<Layer
		File="stg4bg1" NameCommand="%function(self) rb_bg1=self end"
		OnCommand="playcommand,Name;diffusealpha,0;customtexturerect,0,0,3,3;zoom,4;texcoordvelocity,-.2,.1;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;"
	/>
	<Layer
		Type="Quad" NameCommand="%function(self) rb_bgquad2=self end"
		OnCommand="playcommand,Name;diffusealpha,0;stretchto,0,0,SCREEN_WIDTH,SCREEN_HEIGHT;diffuse,0,0,0,0;diffusebottomedge,.5,0,0,0;diffusetopedge,.5,0,0,0;"
	/>
	<Layer Type="ActorFrame" OnCommand="rotationz,60;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;"><children>
		<Layer
			File="stg4bg3" NameCommand="%function(self) rb_bg3=self end"
			OnCommand="playcommand,Name;diffusealpha,0;customtexturerect,0,0,1,3;texcoordvelocity,0,.3;zoomx,0.5;zoomy,1.5;"
		/>
	</children></Layer>
	<Layer Type="ActorFrame" OnCommand="rotationz,-60;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;"><children>
		<Layer
			File="stg4bg3" NameCommand="%function(self) rb_bg4=self end"
			OnCommand="playcommand,Name;diffusealpha,0;customtexturerect,0,0,1,3;texcoordvelocity,0,.3;zoomx,0.5;zoomy,1.5;"
		/>
	</children></Layer>
	<Layer
		File="thunder" NameCommand="%function(self) rb_thunder=self end"
		OnCommand="playcommand,Name;diffusealpha,0;vertalign,top;x,SCREEN_CENTER_X;y,0;"
	/>
	<Layer Type="ActorFrame" OnCommand="bob;effectperiod,3;effectmagnitude,0,6,0;"><children>
	<Layer
		File="raikou" NameCommand="%function(self) rb_raiko=self end"
		OnCommand="playcommand,Name;diffusealpha,0;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;"
	/>
	</children></Layer>

</children> </ActorFrame>