    function mod_perframe_reader(beat,deltatime)
        if beat < 294 then
            for i=1,table.getn(mod_mpf_dai) do
                local a = mod_mpf_dai[i]
                if beat >= a[1] and beat < a[2] then
                    a[3](beat,deltatime);
                end
            end
        elseif beat >= 294 and beat < 583.5 then
			for i=1,table.getn(mod_mpf_hal) do
                local a = mod_mpf_hal[i]
                if beat >= a[1] and beat < a[2] then
