		OnCommand="diffusealpha,1;y,SCREEN_BOTTOM;x,SCREEN_CENTER_X;hidden,0;diffusealpha,1;rotationy,90;zoomz,50;zoom,50;z,-100;
		pulse;effectmagnitude,1,1,1;effectcolor2,0.96,1.02,0.96,1;effectcolor1,1.04,0.98,1.04,1;"/>
	</children> </Layer>
	
</children></Layer>