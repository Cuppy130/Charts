		<Layer File="wave2" NameCommand="%function(self) hp_wave2 = self end" OnCommand="playcommand,Name;diffusealpha,.5;stretchto,0,0,sw,sh" />
		<Layer
			File="arena"
			NameCommand="%function(self) hp_wave4 = self end"
			OnCommand="playcommand,Name;x,SCREEN_WIDTH-256;y,SCREEN_HEIGHT;diffusealpha,0;rotationx,90;zoomx,2;zoomz,1.8;z,256*1.8"
		/>
		
	</children></Layer>
	
	<Layer Type="ActorFrame" InitCommand="%function(self) hp_wavebg2 = self end" OnCommand="fov,60;" ><children>
